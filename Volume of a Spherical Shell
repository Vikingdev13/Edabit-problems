import math
from math import pi


# Create a function that takes r1 and r2 as arguments and returns the
# volume of a spherical shell rounded to the nearest thousandth.
# Examples:
# vol_shell(3, 3) ➞ 0
#
# vol_shell(7, 2) ➞ 1403.245
#
# vol_shell(3, 800) ➞ 2144660471.753

# V = 4 / 3 * pi*(R ^ 3 - r ^ 3)


def vol_shell(r1, r2):
    x = 4 / 3 * pi * (r1 ** 3 - r2 ** 3)
    return abs(round(x, 3))


vol_shell(3,800)